{
  "name": "nodeboot-iam-oauth2-elementary-starter",
  "version": "1.0.3",
  "description": "iam with an basic and minimal oauth2 implementation",
  "main": "src/main/Index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=json-summary mocha  'src/test/**/*.test.js' --exit --require mocha-suppress-logs && npm run badges",
    "badges": "badges4cov --source=nyc_json --nyc_json_file_location=./coverage/coverage-summary.json --output_folder=./coverage",
    "testVerbose": "nyc --reporter=html --reporter=json-summary mocha 'src/test/**/*.test.js' --exit "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodeboot/nodeboot-iam-oauth2-elementary-starter"
  },
  "keywords": [
    "npm",
    "database",
    "starter"
  ],
  "author": "jrichardsz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodeboot/nodeboot-iam-oauth2-elementary-starter/issues"
  },
  "homepage": "https://github.com/nodeboot/nodeboot-iam-oauth2-elementary-starter",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "escape-html": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "sinon": "^13.0.1"
  },
  "devDependencies": {
    "badges4cov": "git+https://github.com/jrichardsz/badges4cov.git",
    "body-parser": "^1.19.2",
    "chai": "4.2.0",
    "express": "^4.17.3",
    "meta-js": "git+https://github.com/jrichardsz/meta-js.git#1.0.6-pre-alpha",
    "mocha": "8.1.3",
    "mocha-suppress-logs": "^0.3.1",
    "nyc": "^15.1.0",
    "supertest": "^6.2.2"
  }
}
